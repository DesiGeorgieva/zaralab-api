{
  "name": "akrabat/slim3-skeleton",
  "description": "Simple Slim Framework 3 skeleton with Twig & Monolog.",
  "license": "BSD-3-Clause",
  "type": "project",
  "keywords": [
    "slim-framework",
    "skeleton"
  ],
  "authors": [
    {
      "name": "Rob Allen",
      "email": "rob@akrabat.com",
      "homepage": "http://akrabat.com"
    }
  ],
  "autoload": {
    "psr-4": {
      "Zaralab\\": "src/Zaralab",
      "Behat\\RestTestingContext\\": "src/Behat/RestTestingContext",
      "Behat\\RestTestingExtension\\": "src/Behat/RestTestingExtension"
    }
  },
  "require": {
    "php": ">=5.4",
    "slim/slim": "^3.0@beta",
    "slim/twig-view": "^1.1",
    "slim/flash": "^0.1.0",
    "monolog/monolog": "^1.13",
    "symfony/console": "^2.7",
    "symfony/finder": "^2.7",
    "doctrine/orm": "^2.5",
    "doctrine/migrations": "^1.0",
    "jms/serializer": "1.*",
    "symfony/security": "^2.7",
    "tuupola/slim-jwt-auth": "^2.0"
  },
  "require-dev": {
    "phpspec/phpspec": "~2@dev",
    "phpspec/prophecy": "~1.0",
    "henrikbjorn/phpspec-code-coverage": "~0.2",
    "nelmio/alice": "2.*",
    "doctrine/data-fixtures": "~1.0",
    "phploc/phploc": "2.*",
    "behat/behat": "~3.0.6",
    "behat/web-api-extension": "~1.0@dev",
    "phpunit/phpunit": "~4.1.0"
  },
  "config": {
    "bin-dir": "bin"
  }
}
